{
  "name": "nest-m-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "lerna run clean --parallel && rimraf dist",
    "build": "lerna run build --stream && nest build",
    "watch": "lerna run clean --parallel && lerna run watch --parallel",
    "start": "NODE_ENV=production node dist/main",
    "start:dev": "lerna run start:dev & NODE_ENV=development nest start --watch",
    "clean": "lerna run clean --parallel && rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"packages/**/*.ts\"",
    "prepack": "yarn run build",
    "publish": "lerna publish",
    "postpublish": "yarn run clean",
    "lint": "eslint \"{lib,src,apps,packages,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-fastify": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "fastify-cookie": "^5.3.1",
    "fastify-static": "^4.0.1",
    "js-yaml": "^4.1.0",
    "mysql2": "^2.2.5",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "vite": "^2.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/js-cookie": "^2.2.6",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.0.3",
    "@types/nunjucks": "^3.1.4",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-legacy": "^1.3.4",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "axios": "^0.21.1",
    "element-plus": "^1.0.2-beta.44",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "ismobilejs": "^1.1.1",
    "jest": "^26.6.3",
    "js-cookie": "^2.2.1",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "prettier": "^2.3.0",
    "rollup": "^2.47.0",
    "sass": "^1.32.13",
    "screenfull": "^5.1.0",
    "scss": "^0.2.4",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "vite-plugin-style-import": "^0.10.0",
    "vite-plugin-svg-sprite": "^0.2.2",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.0",
    "webpack": "^5.37.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/packages/"
    ],
    "moduleNameMapper": {
      "@nestseed/app/(.*)": "<rootDir>/packages/app/src/$1",
      "@nestseed/app": "<rootDir>/packages/app/src"
    }
  }
}
