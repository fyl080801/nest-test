{
  "name": "nest-m-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "lerna run clean --parallel && rimraf dist",
    "build": "lerna run build --stream && nest build",
    "watch": "lerna run clean --parallel && lerna run watch --parallel",
    "start": "NODE_ENV=production node dist/main",
    "start:dev": "lerna run start:dev & NODE_ENV=development nest start --watch",
    "clean": "lerna run clean --parallel && rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"packages/**/*.ts\"",
    "prepack": "yarn run build",
    "publish": "lerna publish",
    "postpublish": "yarn run clean",
    "lint": "eslint \"{lib,src,apps,packages,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-fastify": "^8.0.4",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "fastify-cookie": "^5.3.1",
    "fastify-static": "^4.2.2",
    "js-yaml": "^4.1.0",
    "mysql2": "^2.3.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^6.2.0",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "vite": "^2.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^2.2.7",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.7.10",
    "@types/nunjucks": "^3.1.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-legacy": "^1.5.2",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.9",
    "axios": "^0.21.3",
    "element-plus": "^1.1.0-beta.9",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ismobilejs": "^1.1.1",
    "jest": "^27.1.0",
    "js-cookie": "^3.0.1",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.12",
    "normalize.css": "^8.0.1",
    "prettier": "^2.3.2",
    "rollup": "^2.56.3",
    "sass": "^1.39.0",
    "screenfull": "^5.1.0",
    "scss": "^0.2.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-svg-sprite": "^0.2.3",
    "vue": "^3.2.9",
    "vue-router": "^4.0.11",
    "webpack": "^5.52.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
