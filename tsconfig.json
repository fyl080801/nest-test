{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@seed/app": ["modules/app/src"],
      "@seed/app/*": ["modules/app/src/*"],
      "@seed/vite_service": ["modules/vite_service/src"],
      "@seed/vite_service/*": ["modules/vite_service/src/*"]
    }
  }
}
